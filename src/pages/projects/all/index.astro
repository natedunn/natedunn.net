---
import Grid from '@components/Grid.astro';
import Layout from '@layouts/LayoutDefault.astro';
import { Image } from '@astrojs/image/components';
import ProjectProper from '@components/grid-items/projects/ProjectProperAlt.astro';
import ProjectRadlist from '@components/grid-items/projects/ProjectRadlistAlt.astro';
import ProjectThred from '@components/grid-items/projects/ProjectThred.astro';
import ProjectSlingshot from '@components/grid-items/projects/ProjectSlingshot.astro';
import ProjectApca from '@components/grid-items/projects/ProjectApca.astro';
import Connect from '@components/grid-items/Connect.astro';
---

<Layout title='Projects'>
  <div class='flex flex-col w-full items-start h-full space-y-6'>
    <Grid class='justify-center items-center'>
      <div class='text-3xl text-drop-300 dark:text-drop-700'>
        <svg
          xmlns='http://www.w3.org/2000/svg'
          viewBox='0 0 24 24'
          fill='none'
          stroke='currentColor'
          stroke-width='1.5'
          stroke-linecap='round'
          stroke-linejoin='round'
          class='lucide lucide-sparkle w-10 h-10'
        >
          <path
            d='m12 3-1.9 5.8a2 2 0 0 1-1.287 1.288L3 12l5.8 1.9a2 2 0 0 1 1.288 1.287L12 21l1.9-5.8a2 2 0 0 1 1.287-1.288L21 12l-5.8-1.9a2 2 0 0 1-1.288-1.287Z'
          >
          </path>
        </svg>
      </div>
      <div class='inline-block border-b border-drop-200 dark:border-drop-800 w-full'></div>
    </Grid>
    <Grid>
      <div class='md:w-2/12'>
        <div class='box flex items-center justify-center text-7xl h-full'>
          ðŸ‘‹
          <!-- <Image
            src='https://web.archive.org/web/20090830054951/http://www.geocities.com/iciaos/globe.gif'
            height={100}
            width={100}
            alt='Globe'
          /> -->
        </div>
      </div>
      <div class='md:w-10/12'>
        <div class='box text-3xl h-full flex items-center'>
          <div>
            <span>Here are just a few things</span><br />
            <span class='text-gradient'>Nate</span>
            <span>that has worked on.</span>
          </div>
        </div>
      </div>
    </Grid>
    <Grid class='justify-center items-center'>
      <div class='text-3xl text-drop-300 dark:text-drop-700'>
        <svg class='w-12 h-12' viewBox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg'>
          <path
            d='M4.75 10.25V5.75C4.75 5.19772 5.19772 4.75 5.75 4.75H9.25C9.80228 4.75 10.25 5.19772 10.25 5.75V10.25C10.25 10.8023 9.80228 11.25 9.25 11.25H5.75C5.19772 11.25 4.75 10.8023 4.75 10.25Z'
            stroke='currentColor'
            stroke-width='1.5'
            stroke-linecap='round'
            stroke-linejoin='round'
          >
          </path>
          <path
            d='M4.75 18.25V15.75C4.75 15.1977 5.19772 14.75 5.75 14.75H9.25C9.80228 14.75 10.25 15.1977 10.25 15.75V18.25C10.25 18.8023 9.80228 19.25 9.25 19.25H5.75C5.19772 19.25 4.75 18.8023 4.75 18.25Z'
            stroke='currentColor'
            stroke-width='1.5'
            stroke-linecap='round'
            stroke-linejoin='round'
          >
          </path>
          <path
            d='M13.75 6.25V5.75C13.75 5.19772 14.1977 4.75 14.75 4.75H18.25C18.8023 4.75 19.25 5.19772 19.25 5.75V6.25C19.25 6.80228 18.8023 7.25 18.25 7.25H14.75C14.1977 7.25 13.75 6.80228 13.75 6.25Z'
            stroke='currentColor'
